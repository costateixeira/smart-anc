    <extension url="statement">
      <valueString value="/*&#xA;  Description: The proportion of hepatitis B seroprevalence in the&#xA;    general population&#xA;*/&#xA;define &quot;Population prevalence of hepatitis B&quot;:&#xA;  3 '%'"/>
