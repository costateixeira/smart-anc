  fhir:action ( [
     fhir:title [ fhir:v "ANC.DT.21 Syphilis diagnosis" ] ;
     fhir:trigger ( [
       fhir:type [ fhir:v "named-event" ] ;
       fhir:name [ fhir:v "ANC.B10.4. Diagnosis and treatment" ]
     ] ) ;
     fhir:action ( [
       fhir:id [ fhir:v "1" ] ;
       fhir:title [ fhir:v "Conduct syphilis counselling and further testing" ] ;
       fhir:description [ fhir:v "Conduct syphilis counselling and further testing" ] ;
       fhir:textEquivalent [ fhir:v "Procedure: Provide first dose of treatment and refer for additional testing\n– If primary or secondary stage of syphilis, give single dose of benzathine penicillin 2,400,000 IU\n– If late or unknown stage of syphilis, give one dose of benzathine penicillin 2,400,000 IU weekly for 3 consecutive weeks\n– Advise on treating partner\n– Encourage HIV testing and counselling\n– Reinforce use of condoms\n– Proceed to further testing with an RPR test\n\"In high-prevalence settings, provider-initiated testing and counselling (PITC) for HIV should be considered a routine component of the package of care for pregnant women in all antenatal care settings. In low-prevalence settings, PITC can be considered for pregnant women in antenatal care settings as a key component of the effort to eliminate mother-to-child transmission of HIV, and to integrate HIV testing with syphilis, viral or other key tests, as relevant to the setting, and to strengthen the underlying maternal and child health systems\" (2)" ] ;
       fhir:documentation ( [
         fhir:type [ fhir:v "citation" ] ;
         fhir:label [ fhir:v "WHO ANC recommendations (2016): B.1.7. Human immunodeficiency virus (HIV) and syphilis (3)" ]
       ] ) ;
       fhir:condition ( [
         fhir:kind [ fhir:v "applicability" ] ;
         fhir:expression [
           fhir:description [ fhir:v "(\"Population prevalence of syphilis in pregnant women\" ≥ 5%)\n  AND (\"Syphilis test type\" = \"Rapid syphilis test (RST)\")\n  AND (Syphilis test result\" = \"Syphilis positive\")" ] ;
           fhir:language [ fhir:v "text/cql-identifier" ] ;
           fhir:expression [ fhir:v "Should Conduct syphilis counselling and further testing" ]
         ]
       ] )
     ] [
       fhir:id [ fhir:v "2" ] ;
       fhir:title [ fhir:v "Conduct syphilis counselling and further testing" ] ;
       fhir:description [ fhir:v "Conduct syphilis counselling and further testing 2" ] ;
       fhir:textEquivalent [ fhir:v "Provide treatment according to the clinical phase of syphilis:\n– If primary or secondary stage of syphilis, give single dose of benzathine penicillin 2,400,000 IU\n– If late or unknown stage of syphilis, give one dose of benzathine penicillin 2,400,000 IU weekly for 3 consecutive weeks\n– Advise on treating partner\n– Encourage HIV testing and counselling\n– Reinforce use of condoms\n– Proceed to further testing with an RPR test\n\"In high-prevalence settings, provider-initiated testing and counselling (PITC) for HIV should be considered a routine component of the package of care for pregnant women in all antenatal care settings. In low-prevalence settings, PITC can be considered for pregnant women in antenatal care settings as a key component of the effort to eliminate mother-to-child transmission of HIV, and to integrate HIV testing with syphilis, viral or other key tests, as relevant to the setting, and to strengthen the underlying maternal and child health systems\" (2)" ] ;
       fhir:documentation ( [
         fhir:type [ fhir:v "citation" ] ;
         fhir:label [ fhir:v "WHO ANC recommendations (2016): B.1.7. Human immunodeficiency virus (HIV) and syphilis (3)" ]
       ] ) ;
       fhir:condition ( [
         fhir:kind [ fhir:v "applicability" ] ;
         fhir:expression [
           fhir:description [ fhir:v "(\"Population prevalence of syphilis in pregnant women\" ≥ 5%)\n  AND (\"Syphilis test type\" = \"Rapid plasma reagin (RPR) test\")\n  AND (Syphilis test result\" = \"Syphilis positive\")" ] ;
           fhir:language [ fhir:v "text/cql-identifier" ] ;
           fhir:expression [ fhir:v "Should Conduct syphilis counselling and further testing 2" ]
         ]
       ] )
     ] [
       fhir:id [ fhir:v "3" ] ;
       fhir:title [ fhir:v "Conduct syphilis counselling and further testing" ] ;
       fhir:description [ fhir:v "Conduct syphilis counselling and further testing 3" ] ;
       fhir:textEquivalent [ fhir:v "Procedure:\n– If primary or secondary stage of syphilis, give single dose of benzathine penicillin 2,400,000 IU\n– If late or unknown stage of syphilis, give one dose of benzathine penicillin 2,400,000 IU weekly for 3 consecutive weeks\n– Advise on treating partner\n– Encourage HIV testing and counselling\n– Reinforce use of condoms\n\"In high-prevalence settings, provider-initiated testing and counselling (PITC) for HIV should be considered a routine component of the package of care for pregnant women in all antenatal care settings. In low-prevalence settings, PITC can be considered for pregnant women in antenatal care settings as a key component of the effort to eliminate mother-to-child transmission of HIV, and to integrate HIV testing with syphilis, viral or other key tests, as relevant to the setting, and to strengthen the underlying maternal and child health systems\" (2)" ] ;
       fhir:documentation ( [
         fhir:type [ fhir:v "citation" ] ;
         fhir:label [ fhir:v "WHO ANC recommendations (2016): B.1.7. Human immunodeficiency virus (HIV) and syphilis (3)" ]
       ] ) ;
       fhir:condition ( [
         fhir:kind [ fhir:v "applicability" ] ;
         fhir:expression [
           fhir:description [ fhir:v "(\"Population prevalence of syphilis in pregnant women\" < 5%)\n  AND (\"Syphilis test result\" = \"Syphilis positive\")" ] ;
           fhir:language [ fhir:v "text/cql-identifier" ] ;
           fhir:expression [ fhir:v "Should Conduct syphilis counselling and further testing 3" ]
         ]
       ] )
