     ] ) ;
     fhir:action ( [
       fhir:id [ fhir:v "1" ] ;
       fhir:title [ fhir:v "Conduct clinical enquiry" ] ;
       fhir:description [ fhir:v "Conduct clinical enquiry" ] ;
       fhir:textEquivalent [ fhir:v "Cultures and communities have ways of referring to the problem with other words. It is important to use the words that women themselves use." ] ;
       fhir:documentation ( [
         fhir:type [ fhir:v "citation" ] ;
         fhir:label [ fhir:v "WHO ANC recommendations (2016): B1.3 (2) \n\nIPV handbook (2014) (12)" ]
       ] ) ;
       fhir:condition ( [
         fhir:kind [ fhir:v "applicability" ] ;
         fhir:expression [
           fhir:description [ fhir:v "(Minimum requirements for IPV assessment = TRUE\n)\n  AND (Woman discloses or is suspected to be subjected to intimate partner violence = TRUE)" ] ;
           fhir:language [ fhir:v "text/cql-identifier" ] ;
           fhir:expression [ fhir:v "Should Conduct clinical enquiry" ]
         ]
       ] )
     ] [
       fhir:id [ fhir:v "2" ] ;
       fhir:title [ fhir:v "Conduct clinical enquiry and provide first-line support and care if IPV confirmed" ] ;
       fhir:description [ fhir:v "Conduct clinical enquiry and provide first-line support and care if IPV confirmed" ] ;
       fhir:textEquivalent [ fhir:v "WHO does not recommend universal screening for violence against women attending health care. WHO does encourage health-care providers to raise the topic with women who have injuries or conditions that they suspect may be related to violence.\n\nNever raise the issue of partner violence unless a woman is alone. Even if she is with another woman, that woman could be the mother or sister of an abuser.\n\nIf you do ask her about violence, do it in an empathic, non-judgemental manner. Use language that is appropriate and relevant to the culture and community you are working in. Some women may not like the words “violence” and “abuse”" ] ;
       fhir:documentation ( [
         fhir:type [ fhir:v "citation" ] ;
         fhir:label [ fhir:v "IPV handbook (2014) (15)" ]
       ] ) ;
       fhir:condition ( [
         fhir:kind [ fhir:v "applicability" ] ;
         fhir:expression [
           fhir:description [ fhir:v "(Minimum requirements for IPV assessment = TRUE)\n  AND (\"Presenting signs and symptoms that trigger suspicion of IPV and clinical inquiry\" = \"Ongoing stress\")" ] ;
           fhir:language [ fhir:v "text/cql-identifier" ] ;
           fhir:expression [ fhir:v "Should Conduct clinical enquiry and provide first-line support and care if IPV confirmed" ]
         ]
       ] )
     ] )
  ] ) . # 

