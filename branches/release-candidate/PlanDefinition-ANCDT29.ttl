  fhir:action ( [
     fhir:title [ fhir:v "ANC.DT.29 HIV risk counselling" ] ;
     fhir:trigger ( [
       fhir:type [ fhir:v "named-event" ] ;
       fhir:name [ fhir:v "ANC.B9. Conduct laboratory tests and imaging" ]
     ] ) ;
     fhir:action ( [
       fhir:id [ fhir:v "1" ] ;
       fhir:title [ fhir:v "Conduct counselling on HIV risk" ] ;
       fhir:description [ fhir:v "Conduct counselling on HIV risk" ] ;
       fhir:textEquivalent [ fhir:v "If the woman lives in a setting with a generalized HIV epidemic, or her partner is HIV positive, or she injects drugs, or she is a sex worker, then she is at high risk for HIV – counselling should be given to provide prevention options\n\nProvide comprehensive HIV prevention options: \n– STI screening and treatment (syndromic and syphilis)\n– Condom promotion\n– Risk reduction counselling\n– Pre-exposure prophylaxis (PrEP) with emphasis on adherence\n– Emphasize importance of follow-up ANC contact visits" ] ;
       fhir:documentation ( [
         fhir:type [ fhir:v "citation" ] ;
         fhir:label [ fhir:v "WHO ANC recommendations (2016): B.1.7, C.7 (2)\nPregnancy, childbirth, postpartum and newborn care guide (IMPAC) (2015): C6 (1)" ]
       ] ) ;
       fhir:condition ( [
         fhir:kind [ fhir:v "applicability" ] ;
         fhir:expression [
           fhir:description [ fhir:v "((((((\"HIV status\" = \"HIV negative\")\n  AND (\"Prevalence of HIV in pregnant women in the catchment area\" > 1%))\n  OR ((\"HIV status\" = \"HIV negative\")\n  AND (\"Partner HIV status (reported)\" = \"HIV positive\")))\n  OR ((\"HIV status\" = \"HIV negative\")\n  AND (\"Partner HIV test result\" = HIV positive\")))\n  OR ((\"HIV status\" = \"HIV negative\")\n  AND (Partner HIV status (confirmed)\" = \"HIV positive\")))\n  OR ((\"HIV status\" = \"HIV negative\")\n  AND (Current alcohol and/or other substance use\" = \"Injectable drugs\")))\n  OR ((\"HIV status\" = \"HIV negative\")\n  AND (\"Occupation\" = \"Employment that puts woman at increased risk for HIV (e.g. sex worker)\"))" ] ;
           fhir:language [ fhir:v "text/cql-identifier" ] ;
           fhir:expression [ fhir:v "Should Conduct counselling on HIV risk" ]
         ]
       ] )
     ] [
       fhir:id [ fhir:v "2" ] ;
       fhir:title [ fhir:v "Conduct counselling on pre-exposure prophylaxis (PrEP) for HIV prevention" ] ;
       fhir:description [ fhir:v "Conduct counselling on pre-exposure prophylaxis (PrEP) for HIV prevention" ] ;
       fhir:textEquivalent [ fhir:v "Oral pre-exposure prophylaxis (PrEP) containing tenofovir disoproxil fumarate (TDF) should be offered as an additional prevention choice for pregnant women at substantial risk of HIV infection as part of combination prevention approaches.\n\nSee \"PrEP offering framework\" – a suggested prioritization framework for offering PrEP to pregnant and breastfeeding women (below)." ] ;
       fhir:documentation ( [
         fhir:type [ fhir:v "citation" ] ;
         fhir:label [ fhir:v "WHO ANC recommendations (2016): C.7 (2)\nPre-exposure prophylaxis (PrEP) for HIV prevention (2017) (20)" ]
       ] ) ;
       fhir:condition ( [
         fhir:kind [ fhir:v "applicability" ] ;
         fhir:expression [
           fhir:description [ fhir:v "\"Population incidence of HIV in the absence of PrEP\" > 3 per 100 person-years" ] ;
           fhir:language [ fhir:v "text/cql-identifier" ] ;
           fhir:expression [ fhir:v "Should Conduct counselling on pre-exposure prophylaxis (PrEP) for HIV prevention" ]
         ]
       ] )
     ] [
       fhir:id [ fhir:v "3" ] ;
       fhir:title [ fhir:v "Conduct counselling on HIV risk" ] ;
       fhir:description [ fhir:v "Conduct counselling on HIV risk 2" ] ;
       fhir:textEquivalent [ fhir:v "Encourage woman to find out the status of her partner(s) or to bring them during the next contact visit to get tested" ] ;
       fhir:documentation ( [
         fhir:type [ fhir:v "citation" ] ;
         fhir:label [ fhir:v "WHO ANC recommendations (2016): B.1.7 (2)\nConsolidated guidelines on HIV testing services (2015) (6)" ]
       ] ) ;
       fhir:condition ( [
         fhir:kind [ fhir:v "applicability" ] ;
         fhir:expression [
           fhir:description [ fhir:v "(\"Partner HIV status (reported)\" = \"Inconclusive\")\n  OR (\"Partner HIV test conducted\" = FALSE)" ] ;
           fhir:language [ fhir:v "text/cql-identifier" ] ;
           fhir:expression [ fhir:v "Should Conduct counselling on HIV risk 2" ]
         ]
       ] ) ;
       fhir:action ( [
         fhir:title [ fhir:v "Conduct counselling on HIV risk" ]
       ] [
         fhir:title [ fhir:v "Recommended partner HIV test" ]
       ] )
     ] )
  ] ) . # 

